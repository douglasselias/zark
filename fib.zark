(define fib (lambda n)
  (if (less-than n 2)
      n
      (mul n (fib (sub n 1)))))

(loop (i 3)
  (define (join "vec" (to-string i)) i))

(define factorial (lambda (n)
    (do 
      (define acc n)
      (while (less-than n 2)
        (do (set n (sub n 1))
        (set acc (mul acc n))))
      acc)))